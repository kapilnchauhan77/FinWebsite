<script>
    function to_load_main_quote_graph(company_fincode, type_, DateOption, DateCount, price_to_get='price'){
        moment.suppressDeprecationWarnings = true;
        function volumeInK (value) {
          return value > 1000 ? Math.round(value / 10)/100 + 'K' : value;
        }

        Chart.defaults.LineWithLine = Chart.defaults.line;
        Chart.controllers.LineWithLine = Chart.controllers.line.extend({
            draw: function (ease) {
                Chart.controllers.line.prototype.draw.call(this, ease);
                if (this.chart.tooltip._active && this.chart.tooltip._active.length) {
                    var bty = 0;
                    if (price_to_get=='pe'){
                        bty = this.chart.scales['Price-id'].bottom;
                    }
                    else{
                        bty = this.chart.scales['Volume-id'].bottom;
                    }
                    var activePoint = this.chart.tooltip._active[0],
                        ctx = this.chart.ctx,
                        x = activePoint.tooltipPosition().x,
                        topY = this.chart.scales['Price-id'].top,
                        bottomY = bty;

                    // draw line
                    ctx.save();
                    ctx.beginPath();
                    ctx.moveTo(x, topY);
                    ctx.lineTo(x, bottomY);
                    ctx.lineWidth = 1;
                    ctx.setLineDash([2, 3]),
                        ctx.strokeStyle = '#68696b';
                    ctx.stroke();
                    ctx.restore();
                }
            }
        });

        var currentchart;

        var options;

        var unit_to_use = 'month';
        var _format_ = 'll';
        if (DateOption == "M" || DateOption == "D"){
            unit_to_use = 'day';
        }else if(type_ == "I"){
            unit_to_use = 'minute';
            _format_ = 'lll';
        }

        if (price_to_get == "pe"){
            options = {
                responsive: true,
                scales: {
                    xAxes: [{
                        type: 'time',
                        time: {
                            unit: unit_to_use
                        },
                        gridLines: {
                            drawBorder: false,
                            display: false
                        },
                        ticks: {
                            maxTicksLimit: 6, // hide main x-axis line
                            beginAtZero: true,
                            maxRotation: 0,
                            padding: 10,
                            fontColor: '#000',
                        },
                    }],
                    yAxes: [{
                        display: true,
                        type: 'linear',
                        position: 'right',
                        gridLines: {
                            drawBorder: false,
                            color: '#eeeeee',
                            tickMarkLength: 0,
                            zeroLineColor: '#eeeeee',
                        },
                        scaleLabel: {
                        display: true,
                        labelString: 'Price to Earning',
                        beginAtZero: true,
                        },
                        ticks: {
                            padding: 10
                        },
                        id: 'Price-id'
                    }],
                },
                tooltips: {
                    intersect: false,
                    callbacks: {
                        title: function (tooltipItem, data) {
                            var d = moment(data['labels'][tooltipItem[0]['index']], "M/D/YYYY HH:mm:ss A");
                            return d.format(_format_)
                        },

                        label: function (tooltipItems, data) {
                            var multistringText = [];
                            multistringText.push('PE: ' + data.datasets[0].data[tooltipItems.index]);
                            return multistringText;
                        }
                    },
                    backgroundColor: '#FFFFFF',
                    titleFontSize: 15,
                    titleFontStyle: 'normal',
                    bodyFontFamily: 'Source Sans Pro',
                    bodyFontStyle: 'bold',
                    titleFontFamily: 'Hind',
                    titleFontColor: '#000',
                    bodyFontColor: '#000',
                    bodyFontSize: 18,
                    displayColors: false,
                    xPadding: 10,
                    yPadding: 10,
                    borderColor: '#000',
                    borderWidth: 2,
                },
                legend: {
                    display: false,
                }
            };
        } else{
            options = {
                responsive: true,
                scales: {
