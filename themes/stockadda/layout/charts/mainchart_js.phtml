        var GetChartData = function () {
            var is_it_ytd = false;
            if (typeof(Storage) !== "undefined") {
                var company_graph_price_expired = false;
                var full_data = null;
                // console.log(DateOption);
                full_data = localStorage.getItem(company_fincode + type_ + DateOption + DateCount + price_to_get + "_company_graph_price");
                // console.log(company_fincode + type_ + DateOption + DateCount + price_to_get + "_company_graph_price");
                if (full_data !== null){
                    full_data = JSON.parse(full_data);
                    if (new Date().getTime() > full_data.expiry) {
                        company_graph_price_expired  = true;
                    }
                }
            } else{
                full_data = null;
            }
            if (full_data === null  || company_graph_price_expired === true){
                var count = $('#count').val();
                var type = $('#type').val();
                var StartDate = '';
                if (DateOption == "YTD"){
                    is_it_ytd = true;
                    DateOption = "";
                    StartDate = new Date().getFullYear().toString();
                };
                $.ajax({
                    url: Wo_Ajax_Requests_File(),
                    type: 'GET',
                    dataType: 'json',
                    data: {
                        f:'company_graph_price',
                        fincode: company_fincode,
                        timestamp: Math.round((new Date()).getTime() / 1000),
                        type: type_,
                        DateOption: DateOption,
                        DateCount: DateCount,
                        StartDate: StartDate,
                        EndDate: ''
                    },
                    success: function (data) {
                        d = data.company_prices;
                        console.log(d);
                        var dateLabel = [];
                        var priceValue = [];
                        var volumeValue = [];
                        chartData = {};

                        if (price_to_get == "pe"){
                            $.each(d.Table, function (key, value) {
                                dateLabel.push(value.Date);
                                priceValue.push(Math.round(value.pe * 100) / 100);
                            });
                        } else{
                            $.each(d.Table, function (key, value) {
                                dateLabel.push(value.Date);
                                priceValue.push(Math.round(value.price * 100) / 100);
                                volumeValue.push(value.volume);
                            });
                        }

                        if (price_to_get == "pe"){
                            chartData = {
                                labels: dateLabel,
                                datasets: [
                                    {
                                        label: 'Price',
                                        yAxisID: "Price-id",
                                        data: priceValue,
                                        pointRadius: 0,
                                        pointBackgroundColor: '#000',
                                        pointBorderWidth: 0,
                                        fill: false,
                                        borderColor: 'rgba(24, 68, 93, 1)',
                                        borderWidth: 2,
                                    }
                                ]
                            };
                        } else{
                            chartData = {
                                labels: dateLabel,
                                datasets: [
                                    {
                                        label: 'Price',
                                        yAxisID: "Price-id",
                                        data: priceValue,
                                        pointRadius: 0,
                                        pointBackgroundColor: '#000',
                                        pointBorderWidth: 0,
                                        fill: false,
                                        borderColor: 'rgba(24, 68, 93, 1)',
                                        borderWidth: 2,
                                    },
                                    {
                                        type: 'bar',
                                        maxBarThickness: 5,
                                        label: 'Volume',
                                        yAxisID: "Volume-id",
                                        data: volumeValue,
                                        pointRadius: 0,
                                        pointBackgroundColor: '#03e08b',
                                        pointBorderWidth: 0,
                                        fill: true,
                                        backgroundColor: 'rgba(160, 216, 248, 1)',
                                        borderColor: 'rgba(160, 216, 248, 1)',
                                        borderWidth: 2,
                                    }
                                ]
                            };
                        }
                        if (typeof(Storage) !== "undefined") {
                            var item;
                            item = {
                                value: chartData,
                                expiry: new Date().getTime() + 210000
                            }
                            if (type_=='H'){
                                item = {
									value: chartData,
									expiry: new Date().getTime() + 2100000000000
                                }
                            }
                            if(is_it_ytd == true){
                                DateOption = "YTD";
                            }
                            if (company_graph_price_expired === true){
                                localStorage.removeItem(company_fincode + type_ + DateOption + DateCount + price_to_get + "_company_graph_price");
                            }
                            localStorage.setItem(company_fincode + type_ + DateOption + DateCount + price_to_get + "_company_graph_price", JSON.stringify(item));
							}
                        respondCanvas();
                    }
                });
            }else{
                chartData = full_data.value;
                respondCanvas();
            }
        };

        function respondCanvas() {
            if (document.getElementById("dual-line") != null) {
                $('#loading').html('');
                var ctx = document.getElementById("dual-line").getContext("2d");
                var chart = new Chart(ctx, {
                    type: 'LineWithLine',
                    data: chartData,
                    options: options,
                });

                if ($('#type').val() == "w" || ($('#type').val() == "m" && $('#count').val() == "1")) {
                    chart.options.scales.xAxes[0].time.unit = 'day';
                    chart.update();
                }
                currentchart = chart;

            }
        }


        $(document).ready(function () {
            if (company_fincode==""){
                company_fincode = <?php echo $wo['company_data']['fincode']; ?>;
            }
            GetChartData();
        });


        if (window.matchMedia("(max-width: 575px)").matches) {
            $("#dual-line").attr("height", "960");
        }

    }

